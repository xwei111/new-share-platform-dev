{
  "presets": [
    "react", 
    ["es2015", {"modules": false}], 
    "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy", 
    ["import", {"libraryName": "antd", "style": true}]
  ],
  "env": {
    "dev": {
      "presets": ["react-hmre"]
    },
    "prod": {
      "plugins": ["lodash"]
    }
  }
}
